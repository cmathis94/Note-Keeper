<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Notes</title>
</head>

<body>
    <h1>Notes</h1>

    <div>
        <h2>
            <% for(let note of notes) { %>
                <%=note.title%>
                    <% } %>
        </h2>
        <ul>
            <% for(let note of notes) { %>
                <li>
                    <%=note.note%>
                        <ul>
                            <li>
                                <%=note.subNote%>
                            </li>
                        </ul>
                </li>
                <% } %>
        </ul>
    </div>

    <div>
        <form action="/notes" method="post">
            <div>
                <input type="text" name="note" id="note" placeholder="Type note here...">
            </div>
            <div>
                <input type="text" name="subNote" id="subNote" placeholder="Type subnote here...">
            </div>
            <button>Submit</button>
        </form>
    </div>
</body>

</html>